URxvt*perl-lib: /home/luis/.urxvt/:/home/luis/.urxvt/urxvt-vim-scrollback
URxvt*perl-ext-common: default,tabbed,matcher,resize-font,vim-scrollback
URxvt.font: xft:Gohu GohuFont:size=11
URxvt.secondaryWheel: 1
URxvt.scrollBar: false
URxvt.hold: false

! URL options
URxvt.urlLauncher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.underlineURLs: True

! Vim-Scrollback
URxvt.vim-scrollback: M-q
URxvt.vim-scrollback-bg: 16
URxvt.vim-scrollback-fg: 10

! Borders
URxvt.internalBorder: 20
URxvt.externalBorder: 10
URxvt.borderLess: false

! Disable default binding 
URxvt.iso14755: false
URxvt.iso14755_52: false

! Cursor
URxvt.cursorBlink:          true
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank: true

! Color Schemes !

! Dracula 
*.foreground:   #f8f8f2
*.background: #0E181A

*.color0:       #6272a4
*.color1:       #ff5555
